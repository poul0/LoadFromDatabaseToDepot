                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[dbo].[Payments]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    p.payment_id,                                   -- ID платежу
    p.schedule_id,                                  -- ID розкладу платежів
    c.client_id,                                    -- ID клієнта
    cr.credit_id,                                   -- ID кредиту
    p.payment_date,                                 -- Дата платежу
    p.principal_paid,                               -- Сплачена сума основного боргу
    p.interest_paid,                                -- Сплачена сума процентів
    p.penalty_paid,                                 -- Сплачена сума пені
    CASE 
        WHEN (cr.amount - SUM(p.principal_paid) OVER (PARTITION BY cr.credit_id)) &lt; 0 
        THEN 0  -- Якщо залишок заборгованості від’ємний, встановлюємо 0
        ELSE (cr.amount - SUM(p.principal_paid) OVER (PARTITION BY cr.credit_id))
    END AS outstanding_balance,                    -- Залишок заборгованості
    t.full_date,                                    -- Поле full_date із таблиці DimTime
    t.time_id                                       -- Поле time_id із таблиці DimTime
FROM 
    [Credit].[dbo].[Payments] p
JOIN 
    [Credit].[dbo].[RepaymentSchedule] rs ON p.schedule_id = rs.schedule_id  -- З’єднання з розкладом платежів
JOIN 
    [Credit].[dbo].[Credits] cr ON rs.credit_id = cr.credit_id               -- З’єднання з кредитами
JOIN 
    [Credit].[dbo].[Clients] c ON cr.client_id = c.client_id                 -- З’єднання з клієнтами
JOIN 
    [CreditDW].[dbo].[DimTime] t ON p.payment_date = t.full_date             -- З’єднання з таблицею DimTime
GROUP BY
    p.payment_id,
    p.schedule_id,
    c.client_id,
    cr.credit_id,
    p.payment_date,
    p.principal_paid,
    p.interest_paid,
    p.penalty_paid,
    cr.amount,
    t.full_date,
    t.time_id; -- Додаємо time_id до GROUP BY</property>
